{
  "name": "gw-gatsby-sanity-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "gatsby develop",
    "build": "npm run build:sanity && npm run build:web",
    "build:web": "cd web && gatsby build",
    "start:web": "cd web && gatsby start",
    "build:sanity": "cd studio && npx sanity build ../public/studio -y && cd ..",
    "start:sanity": "cp .env.local ./studio/.env.development && cd studio && sanity start",
    "postinstall": "lerna bootstrap"
  },
  "dependencies": {
    "lerna": "^3.22.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-json-pretty": "^2.2.0",
    "sanity": "^2.2.6"
  }
}
