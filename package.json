{
  "private": true,
  "name": "gw-gatsby-sanity-base",
  "version": "1.0.4",
  "scripts": {
    "build": "sanity-template build --output public --template-values .sanity-template/template-values.json && cd studio && lerna run graphql-deploy",
    "dev": "sanity-template watch --template-values .sanity-template/template-values.json",
    "start": "lerna run dev --parallel",
    "format": "lerna run format",
    "graphql-deploy": "lerna run graphql-deploy",
    "lint": "lerna run lint",
    "postinstall": "lerna bootstrap",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@sanity/cli": "^2.10.5",
    "lerna": "^3.22.1",
    "sanity-template": "^2.4.3"
  },
  "dependencies": {
    "@sanity/production-preview": "^2.2.6"
  }
}
