{
  "version": 2,
  "title": "GW Gatsby Sanity Base",
  "description": "For hot sites",
  "previewMedia": {
    "type": "image",
    "src": ".sanity-template/assets/frontend.png",
    "alt": "Preview Image"
  },
  "technologies": [
    {
      "id": "vercel",
      "name": "Vercel",
      "url": "https://vercel.com/"
    },
    {
      "id": "gatsbyjs",
      "name": "Gatsby",
      "url": "https://gatsbyjs.com"
    }
  ],
  "deployment": {
    "provider": "vercel",
    "studio": {
      "basePath": "/"
    },
    "envVars": {
      "dataset": ["SANITY_PROJECT_DATASET"],
      "projectId": ["SANITY_PROJECT_ID"]
    },
    "corsOrigins": [
      {
        "origin": "http://localhost:8000",
        "allowCredentials": true
      }
    ]
  }
}
