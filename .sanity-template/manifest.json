{
  "version": 2,
  "title": "Gatsby Barebones Starter",
  "description": "Minimal and barebones example of a starter for deployment on Vercel with Gatsby",
  "previewMedia": {
    "type": "image",
    "src": "assets/frontend.png",
    "alt": "Gatsby frontend with Sanity data displaying"
  },
  "technologies": [
    {
      "id": "vercel",
      "name": "Vercel",
      "url": "https://vercel.com/"
    },
    {
      "id": "gatsbyjs",
      "name": "Gatsby",
      "url": "https://gatsbyjs.com"
    }
  ],
  "deployment": {
    "provider": "vercel",
    "studio": { "basePath": "/studio" },
    "corsOrigins": [
      {
        "origin": "http://localhost:3000",
        "allowCredentials": true
      }
    ]
  }
}