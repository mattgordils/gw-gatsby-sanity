{
	"builds": [
		{"config": { "distDir": "studio", "use": "@vercel/node" }},
		{ "src": "*.html", "use": "@vercel/static" },
    { "src": "*.py", "use": "@vercel/python" },
    { "src": "*.js", "use": "@vercel/node" }
	],
	"rewrites": [{ "source": "/studio/(.*)", "destination": "/studio/index.html" }]
}